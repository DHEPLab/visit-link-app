{
  "cli": {
    "version": ">= 11.0.1"
  },
  "build": {
    "production": {
      "env": {
        "EXPO_PUBLIC_APP_NAME": "Healthy Future",
        "EXPO_PUBLIC_APP_VERSION": "4.0.0",
        "EXPO_PUBLIC_API_URL": "https://healthy-future.dhep.org",
        "EXPO_PUBLIC_ANDROID_PACKAGE": "edu.stanford.fsi.reap.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APP_NAME": "Healthy Future(Preview)",
        "EXPO_PUBLIC_APP_VERSION": "4.0.0-preview",
        "EXPO_PUBLIC_API_URL": "https://healthy-future.dhep.org",
        "EXPO_PUBLIC_ANDROID_PACKAGE": "edu.stanford.fsi.reap.apppreview"
      }
    },
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APP_NAME": "Healthy Future(Dev)",
        "EXPO_PUBLIC_APP_VERSION": "4.0.0-dev",
        "EXPO_PUBLIC_API_URL": "https://healthy-future.dhep.org",
        "EXPO_PUBLIC_ANDROID_PACKAGE": "edu.stanford.fsi.reap.appdev"
      }
    }
  },
  "submit": {
    "production": {}
