{
  "name": "healthy-future-app",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --config app.local.json",
    "export:dev": "expo export --config app.dev.json -p https://dev.healthyfutures.cloud/expo --output-dir dist.dev",
    "export:stg": "expo export --config app.stg.json -p https://stg.healthyfutures.cloud/expo --output-dir dist.stg",
    "export:prod": "expo export --config app.prod.json -p https://healthyfutures.cloud/expo --output-dir dist.prod",
    "android": "expo start --android",
    "eject": "expo eject",
    "test": "jest --watch",
    "test:ci": "jest --silent --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
